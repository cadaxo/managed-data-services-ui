{
  "_version": "1.21.0",
  "sap.app": {
    "id": "com.cadaxo.cmds.PrototypeMDS",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/CADAXO/MDS_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "toAllLinks": {
        "uri": "localService/mockdata/toAllLinks.json",
        "type": "JSON"
      },
      "step2Model": {
        "uri": "localService/mockdata/step2Model.json",
        "type": "JSON"
      },
      "step2ModelLinks": {
        "uri": "localService/mockdata/step2ModelLinks.json",
        "type": "JSON"
      },
      "step3Model": {
        "uri": "localService/mockdata/step3Model.json",
        "type": "JSON"
      },
      "step3ModelLinks": {
        "uri": "localService/mockdata/step3ModelLinks.json",
        "type": "JSON"
      } 
    }   
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.cadaxo.cmds.view.App",
      "type": "XML",
      "async": true,
      "id": "idAppControl"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.cadaxo.cmds.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService"
      },
      "toAllLinks" :{
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "toAllLinks"
      },
      "step2Model" :{
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "step2Model"
      },
      "step2ModelLinks" :{
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "step2ModelLinks"
      },
      "step3Model" :{
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "step3Model"
      },
      "step3ModelLinks" :{
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "step3ModelLinks"
      }             
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.cadaxo.cmds.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": "RouteApp",
          "target": [
            "TargetApp"
          ]
        }
      ],
      "targets": {
        "TargetMainView": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewId": "app",
          "viewName": "App"
        }
      }
    }
  }
}
